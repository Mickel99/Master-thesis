{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productImg": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    },
    "productName": {
      "type": "string",
      "required": true
    },
    "productPrice": {
      "type": "decimal",
      "required": true
    },
    "productDescription": {
      "type": "text",
      "required": true
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "Cases",
        "Screen protector",
        "Charger"
      ],
      "required": true
    }
  }
}
